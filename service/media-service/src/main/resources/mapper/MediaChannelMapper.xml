<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.fzdkx.media.mapper.MediaChannelMapper">

    <select id="selectChannelList" resultType="com.fzdkx.model.media.bean.MediaChannel">
        select * from wm_channel
    </select>
    <select id="selectById" resultType="com.fzdkx.model.media.bean.MediaChannel">
        select * from wm_channel
        where id = #{id}
    </select>
</mapper>